<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>BigEgg Framework Collection Changed Event Snippet</Title>
      <Shortcut>becolleche</Shortcut>
      <Description>Code snippet for Event which collection changed</Description>
      <Author>BigEgg</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>name</ID>
          <ToolTip>method name</ToolTip>
          <Default>Model</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp">
        <![CDATA[private void $name$CollectionChanged(object sender, NotifyCollectionChangedEventArgs e)
    {
        $selected$$end$
    }]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>